# Iterative fibonacci
fn fib(num: Int) -> Int:
  if num < 1:
    return 0
  end
  var n1 = 0
  var n2 = 1
  var i = 1
  while i < num:
    let next = n2 + n1
    n1 = n2
    n2 = next
    i = i + 1
  end
  return n2
end

var i = 1
while i <= 10:
  println(str(i) + ": " + str(fib(i)))
  i = i + 1
end

